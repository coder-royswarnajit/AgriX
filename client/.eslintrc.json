{
    "extends": [
        "react-app",
        "react-app/jest"
    ],
    "rules": {
        "no-unused-vars": "warn",
        "react/jsx-pascal-case": "off",
        "react-hooks/exhaustive-deps": "warn",
        "eqeqeq": "warn",
        "array-callback-return": "warn",
        "no-lone-blocks": "warn",
        "jsx-a11y/alt-text": "warn",
        "jsx-a11y/heading-has-content": "warn",
        "jsx-a11y/scope": "warn",
        "no-dupe-keys": "warn"
    }
}